FILE = TEXT | FOLD | LINK

TEXT = {
    type: "text",
    name: NAMESTR,
    text: string(any)
}
FOLD = {
    type: "fold",
    name: NAMESTR,
    contents: [ FILE ]
}
LINK = {
    type: "link",
    name: NAMESTR,
    hard: true | false,
    path: /NAMESTR/NAMESTR...
}

NAMESTR = string(/^[\w\.][\w\.-]*$/)
VARSTR = string(/^[a-zA-Z_\.][\w\.]*$/)
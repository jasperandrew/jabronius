FILE = DATA | FOLD | LINK

DATA = {
    type: "data",
    name: NAMESTR,
    data: string(any)
}
FOLD = {
    type: "fold",
    name: NAMESTR,
    contents: [ FILE ]
}
LINK = {
    type: "link",
    name: NAMESTR,
    path: /NAMESTR/NAMESTR...
}

NAMESTR = string(/^[\w\.][\w\.-]*$/)
VARSTR = string(/^[a-zA-Z_\.][\w\.]*$/)
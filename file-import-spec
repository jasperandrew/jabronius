FILE = TEXT | FOLD | FUNC | LINK
NAMESTR = string(/^[\w\.][\w\.-]*$/)
VARSTR = string(/^[a-zA-Z_\.][\w\.]*$/)

TEXT = {
    name: NAMESTR,
    type: "text",
    text: string(any)
}
FOLD = {
    name: NAMESTR,
    type: "fold",
    contents: [ FILE ]
}
FUNC = {
    name: NAMESTR,
    type: "func",
    args: [ 'argc', 'argv'],
    body: string
}
LINK = {
    name: NAMESTR,
    type: "link",
    path: /NAMESTR/NAMESTR...
}

IMPORT = [ FILE ]